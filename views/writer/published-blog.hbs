<div class="card shadow">
  <h4 class="card-header">
    Bài viết đã xuất bản
  </h4>
  <div class="card-body">
    <table class="table">
      <thead>
        <tr>
          <th scope="col" width="10%">ID</th>
          <th scope="col" width="30%">Tiêu đề</th>
          <th scope="col" width="25%">Ngày đăng</th>
          <th scope="col" width="25%">Ngày tạo</th>
        </tr>
      </thead>
      <tbody>
        {{#each blogs}}
        <tr>
          <td>{{IDBlog}}</td>
          <td>{{Tittle}}</td>
          <td>{{DateProduct}}</td>
          <td>{{DatePublic}}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>